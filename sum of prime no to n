#include <stdio.h>

int main()
{
    int i,a,n,count,sum=0;
    
    printf("\n enter the range upto which you want to print prime no");
    scanf("%d",&n);
    
    for(i=2 ; i<=n; i++)
    {
        count=0;
        for(a=2 ; a<i ; a++)
        {
            if(i%a==0)
            {
                count++;
            }
            
        }
if(count==0)
{
    sum=sum+i;
}
}

    printf("%d",sum);
}

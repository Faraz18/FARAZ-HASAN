#include <stdio.h>

int main()
{
    int i,n,arr[20];
    printf("enter the number of elements for  array:");
    scanf("%d",&n);
    printf("\n enter the elements for array:");
    for (i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        
    }
    printf("\nthe array is:");
    for (i=0;i<n;i++)
    {
    printf("\n array[%d]=%d ",i,arr[i]);
    }

    return 0;
}

#include <stdio.h>

void main()
{
    int num,a, rev=0,ori; 
    
    printf("\n enter the number");
    scanf("%d",&num);
    ori= num;
    while(num!=0)
    {
    a=num%10;
    rev= rev*10 + a;
    num/=10;
    }
    if(ori == rev)
    
        printf("%d is a palindrome",ori);
    
    else
    
        printf("%d is not a palindrome",ori);
    
}

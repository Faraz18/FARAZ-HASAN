#include <stdio.h>

int main()
{
   int n,ori,a,c=0;
   printf("\n enter the number \n");
   scanf("%d",&n);
   
   ori= n;
   
   while(ori!=0)
   {
       a=ori%10;
       c= c+a*a*a;
       ori/=10;
   }
   if (c==n)
   {
       printf("\n this is armstrong");
   }
   else
   {
       printf("\n this is not armstrong");
   }
   return 0;
}
